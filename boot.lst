  189 A:a13a                                    ; --------------------------------------------------------
  190 A:a13a                                    ; This installs the patches to link the CMDR-DOS into the
  191 A:a13a                                    ; PET BASIC 4 ROM. It requires that the DOS "rom.bin" is
  192 A:a13a                                    ; loaded into the bank 7 (14 as value for $e802) at $4000.
  193 A:a13a                                    ; Also, it must run in the upper half of bank 0, as it
  194 A:a13a                                    ; switches the lower half to bank 7, to run the dos_init.
  195 A:a13a                                    ; 
  196 A:a13a                                    ; It also requires the value of "SDC_COMPANION" to be set
  197 A:a13a                                    ; to the place in the ROM where to copy the link code.

  199 A:a13a                                     .( 

  201 A:a13a                                    ; address of the DOS in DOSBANK
  202 A:a13a                                    DOS=$4000

  204 A:a13a                                    ; DOS jumptable
  205 A:a13a                                    dos_seclstn=DOS+3*0
  206 A:a13a                                    dos_sectlk=DOS+3*1
  207 A:a13a                                    dos_iecin=DOS+3*2
  208 A:a13a                                    dos_iecout=DOS+3*3
  209 A:a13a                                    dos_untlk=DOS+3*4 ; note: currently no-op
  210 A:a13a                                    dos_unlsn=DOS+3*5
  211 A:a13a                                    dos_listn=DOS+3*6
  212 A:a13a                                    dos_talk=DOS+3*7 ; note: currently same as listn

  214 A:a13a                                    dos_init=DOS+3*8

  216 A:a13a                                    ; banking

  219 A:a13a                                     .assert BANK=$e802,"BANK register incorrectly defined"

  225 A:a13a                                    DOSBANK=15           ; DOS runs in memory mapped from $078000-$07ffff
  226 A:a13a                                    BASBANK=0           ; BASIC runs in bank 0

  228 A:a13a                                    ; variables in BASIC address space

  230 A:a13a                                    flags=$03fe           ; for testing for now

  232 A:a13a                                    curdev=$d4           ; current device (unit) number
  233 A:a13a                                    cursec=$d3           ; current secondary address

  235 A:a13a                                    status=$96           ; IEEE status

  237 A:a13a                                    ; DOS bank vars

  239 A:a13a                                    dos_status=$0287           ; from dos inc/banks.inc

